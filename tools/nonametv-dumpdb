#!/bin/sh


TMPDIR=`mktemp -d`
chmod og+rwx $TMPDIR
mysqldump -uroot --add-drop-table --no-data listings batches channels files programs state trans_cat languagestrings > $TMPDIR/listings.sql
mysqldump -uroot --no-create-info -T $TMPDIR --fields-terminated-by=, --fields-optionally-enclosed-by=\" listings channels trans_cat languagestrings
mv $TMPDIR/* .
rmdir $TMPDIR
