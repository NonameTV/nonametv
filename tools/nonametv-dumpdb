#!/bin/sh


TMPDIR=`mktemp -d`
chmod og+rwx $TMPDIR
mysqldump -uroot --add-drop-table --no-data listings > $TMPDIR/listings.sql
mysqldump -uroot --no-create-info -T $TMPDIR --fields-terminated-by=, --fields-optionally-enclosed-by=\" listings channels trans_cat
mysqldump -uroot --complete-insert --extended-insert listings lang_en lang_hr lang_se > $TMPDIR/languages.sql
mv $TMPDIR/* .
rmdir $TMPDIR
